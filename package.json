{
  "name": "gym-pass-api",
  "version": "1.0.0",
  "description": "An API for managing gym access, utilizing design patterns and SOLID principles.",
  "main": "server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfredo-petri/gym-pass-api.git"
  },
  "keywords": [
    "rest",
    "api",
    "fastify",
    "solid",
    "design",
    "pattern"
  ],
  "author": "Alfredo Augusto Petri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alfredo-petri/gym-pass-api/issues"
  },
  "homepage": "https://github.com/alfredo-petri/gym-pass-api#readme",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/node": "22.14.1",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "8.57.1",
    "prisma": "6.6.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  },
  "dependencies": {
    "@prisma/client": "6.6.0",
    "@types/bcryptjs": "2.4.6",
    "bcryptjs": "3.0.2",
    "dotenv": "16.5.0",
    "fastify": "5.3.0",
    "zod": "3.24.2"
  }
}
